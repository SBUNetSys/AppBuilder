<?xml version="1.0" encoding="utf-8"?>
<android.support.wearable.view.BoxInsetLayout>
    <FrameLayout>
        <!--from none (or no from attribute) means not from any phone viewId-->
        <TextView from="none">
            <text>Recently Played</text>
        </TextView>
    </FrameLayout>
    <!--for list-like view, from value comes from the item root layout view id-->
    <android.support.wearable.view.WearableListView from="com.spotify.music:id/row_view"/>
</android.support.wearable.view.BoxInsetLayout>